{
   "name": "food_service",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "db": "typeorm",
      "clean": "rimraf build",
      "compile": "tsc",
      "compile_dev": "tsc -w",
      "start": "npm run clean && npm run compile && pm2-runtime start build/index.js",
      "dev": "nodemon -e ts --ignore \"./tests\" --exec \"npm run start\"",
      "test": "NODE_ENV=test mocha --timeout 15000 -r ts-node/register tests/**/*.spec.ts",
      "unit_test": "NODE_ENV=test mocha -r ts-node/register tests/unit/**/*.spec.ts",
      "integration_test": "NODE_ENV=test mocha -r ts-node/register tests/integration/**/*.spec.ts",
      "integration_db_test": "NODE_ENV=test mocha -r ts-node/register tests/integration/database/**/*.spec.ts",
      "integration_http_test": "NODE_ENV=test mocha -r ts-node/register tests/integration/http/**/*.spec.ts",
      "docker_test": "docker exec -it food_service npm test",
      "docker_unit_test": "docker exec -it food_service npm run unit_test",
      "docker_integration_test": "docker exec -it food_service npm run integration_test",
      "docker_integration_db_test": "docker exec -it food_service npm run integration_db_test"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "dependencies": {
      "@bit/domiebett.budget_app.ip-resolver": "^1.0.0",
      "@bit/domiebett.budget_app.jwt-authenticate": "^1.1.2",
      "@bit/domiebett.budget_app.logging": "^1.0.0",
      "body-parser": "^1.19.0",
      "config": "^3.2.2",
      "cors": "^2.8.5",
      "dotenv": "^8.1.0",
      "eureka-js-client": "^4.5.0",
      "express": "^4.17.1",
      "mysql2": "^1.7.0",
      "pm2": "^3.5.1",
      "reflect-metadata": "^0.1.10",
      "rimraf": "^2.7.1",
      "routing-controllers": "^0.7.7",
      "spdy": "^4.0.1",
      "swagger-ui-express": "^4.1.1",
      "typedi": "^0.8.0",
      "typeorm": "0.2.18",
      "winston": "^3.2.1"
   },
   "devDependencies": {
      "@types/body-parser": "^1.17.1",
      "@types/chai": "^4.2.3",
      "@types/chai-as-promised": "^7.1.2",
      "@types/config": "0.0.34",
      "@types/cors": "^2.8.6",
      "@types/dotenv": "^6.1.1",
      "@types/eureka-js-client": "^4.4.2",
      "@types/express": "^4.17.1",
      "@types/mocha": "^5.2.7",
      "@types/node": "^8.10.54",
      "@types/spdy": "^3.4.4",
      "@types/supertest": "^2.0.8",
      "supertest": "^4.0.2",
      "@types/swagger-ui-express": "^3.0.1",
      "@types/yamljs": "^0.2.30",
      "chai": "^4.2.0",
      "chai-as-promised": "^7.1.1",
      "mocha": "^6.2.1",
      "nodemon": "^1.19.3",
      "ts-node": "3.3.0",
      "typescript": "^3.6.3"
   },
   "bit": {
      "env": {
         "compiler": "bit.envs/compilers/typescript@3.0.5"
      },
      "componentsDefaultDirectory": "components/{name}",
      "packageManager": "npm"
   }
}
